@model MyElearningProject.DAL.Entites.About
@{
    ViewBag.Title = "AddAbout";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}


<h2>Hakkımda Ekleme Sayfası</h2>

<form method="post">
    <label>Adınız Soyadınız</label>
    @Html.TextBoxFor(x => x.AboutName, new { @class = "form-control" })
    <br /><br />
    <label>Başlığınızı Giriniz</label>
    @Html.TextBoxFor(x => x.Title, new { @class = "form-control" })
    <br /><br />
    <label>1.Açıklamanız</label>
    @Html.TextBoxFor(x => x.Description1, new { @class = "form-control" })
    <br /><br />
    <label>2.Açıklamanız </label>
    @Html.TextBoxFor(x => x.Description2, new { @class = "form-control" })
    <br /><br />
    <button class="btn btn-primary"> Kaydet</button>
</form>
